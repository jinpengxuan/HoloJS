<Window x:Class="webgl_code_generator.WebAudioGenerator"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:webgl_code_generator"
        mc:Ignorable="d"
        Title="WebAudioGenerator" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Width="100" Height="50" Content="Generate header" Click="Generate_Click"/>
        <TextBox Grid.Row="1" x:Name="Text" Text="asdfasdf" TextWrapping="Wrap" AcceptsReturn="True"/>
    </Grid>
</Window>
